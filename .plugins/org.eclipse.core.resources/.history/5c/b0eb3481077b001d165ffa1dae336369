package controlstructures;

public class ContinueDemo {
	
	

	public static void main(String[] args) {
		String[] array= {"surendra","vinod","satish","sunil"};
		
		for(String demo:array) {
			if(demo.equals("surendra")||demo.equals("sunil")) {
				continue;
			}
			System.out.println(demo);
			
		}
		
	}

}
